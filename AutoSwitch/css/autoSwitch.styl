border-radius()
  -webkit-border-radius arguments
  -moz-border-radius arguments
  border-radius arguments

body
   font: 12px/1.5 'Microsoft Yahei', 'Courier New'
body, ul, p, img, h1, h2, h3
      padding: 0
      margin: 0
.autoSwitchContainer
   width: 1000px
   height: 200px
   margin: 0 auto
   position: relative
   .zjdgx-auto-switch
      width: 100%
      height: 100%
      overflow: hidden
      position: relative
      li
         top: 0
         list-style: none
         position: absolute
   .opt
      left: 0
      width: 100%
      bottom: 10px
      position: absolute
      text-align: center
      a
         width: 12px
         height: 12px
         margin: 0 5px
         border-radius(50%)
         display: inline-block
         background-color: #fff
      .cur
         background-color: #23C778
